{% doc %}
  @prompt
    Create a premium hero carousel section with 4 slides featuring luxury silk saree products. Include full-width responsive images, elegant text overlays with headings and subheadings, call-to-action buttons, autoplay functionality, navigation dots and arrows. The design should be clean, modern and premium with smooth transitions. Allow customization of images, text, button labels and links for each slide through the theme editor., Create a premium hero carousel section with 4 slides featuring luxury silk saree products. Include full-width responsive images, elegant text overlays with headings and subheadings, call-to-action buttons, autoplay functionality, navigation dots and arrows. The design should be clean, modern and premium with smooth transitions. Allow customization of images, text, button labels and links for each slide through the theme editor.
{% enddoc %}
{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-hero-carousel-{{ ai_gen_id }} {
    position: relative;
    width: 100%;
    overflow: hidden;
    background-color: {{ block.settings.background_color }};
  }

  .ai-hero-carousel-slides-{{ ai_gen_id }} {
    position: relative;
    width: 100%;
    height: {{ block.settings.slide_height_desktop }}px;
  }

  .ai-hero-carousel-slide-{{ ai_gen_id }} {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    opacity: 0;
    visibility: hidden;
    transition: opacity {{ block.settings.transition_speed }}ms ease-in-out;
  }

  .ai-hero-carousel-slide-{{ ai_gen_id }}.active {
    opacity: 1;
    visibility: visible;
    z-index: 1;
  }

  .ai-hero-carousel-slide-image-{{ ai_gen_id }} {
    width: 100%;
    height: 100%;
    object-fit: cover;
    display: block;
  }

  .ai-hero-carousel-slide-placeholder-{{ ai_gen_id }} {
    width: 100%;
    height: 100%;
    background-color: #f4f4f4;
    display: flex;
    align-items: center;
    justify-content: center;
    position: relative;
  }

  .ai-hero-carousel-slide-placeholder-{{ ai_gen_id }} svg {
    width: 100%;
    height: 100%;
    max-width: 500px;
    max-height: 500px;
  }

  .ai-hero-carousel-empty-state-{{ ai_gen_id }} {
    position: absolute;
    bottom: 20px;
    left: 50%;
    transform: translateX(-50%);
    padding: 12px 20px;
    font-size: 14px;
    color: #666;
    text-align: center;
    pointer-events: none;
  }

  .ai-hero-carousel-overlay-{{ ai_gen_id }} {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: linear-gradient(
      to bottom,
      rgba(0, 0, 0, {{ block.settings.overlay_opacity | divided_by: 100.0 }}) 0%,
      rgba(0, 0, 0, {{ block.settings.overlay_opacity | times: 0.7 | divided_by: 100.0 }}) 100%
    );
    z-index: 2;
  }

  .ai-hero-carousel-content-{{ ai_gen_id }} {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    text-align: {{ block.settings.text_alignment }};
    z-index: 3;
    width: 90%;
    max-width: {{ block.settings.content_max_width }}px;
    color: {{ block.settings.text_color }};
  }

  .ai-hero-carousel-heading-{{ ai_gen_id }} {
    font-size: {{ block.settings.heading_size }}px;
    font-weight: 700;
    margin: 0 0 16px;
    line-height: 1.2;
    color: {{ block.settings.text_color }};
  }

  .ai-hero-carousel-subheading-{{ ai_gen_id }} {
    font-size: {{ block.settings.subheading_size }}px;
    margin: 0 0 24px;
    line-height: 1.5;
    color: {{ block.settings.text_color }};
    opacity: 0.9;
  }

  .ai-hero-carousel-button-{{ ai_gen_id }} {
    display: inline-block;
    padding: {{ block.settings.button_padding_vertical }}px {{ block.settings.button_padding_horizontal }}px;
    background-color: {{ block.settings.button_background }};
    color: {{ block.settings.button_text_color }};
    text-decoration: none;
    border-radius: {{ block.settings.button_border_radius }}px;
    font-size: {{ block.settings.button_font_size }}px;
    font-weight: 600;
    transition: all 0.3s ease;
    border: none;
    cursor: pointer;
  }

  .ai-hero-carousel-button-{{ ai_gen_id }}:hover {
    background-color: {{ block.settings.button_hover_background }};
    color: {{ block.settings.button_hover_text_color }};
    transform: translateY(-2px);
  }

  .ai-hero-carousel-nav-{{ ai_gen_id }} {
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    z-index: 4;
    background-color: {{ block.settings.arrow_background }};
    border: none;
    width: {{ block.settings.arrow_size }}px;
    height: {{ block.settings.arrow_size }}px;
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    cursor: pointer;
    transition: all 0.3s ease;
    opacity: {{ block.settings.arrow_opacity | divided_by: 100.0 }};
  }

  .ai-hero-carousel-nav-{{ ai_gen_id }}:hover {
    opacity: 1;
    transform: translateY(-50%) scale(1.1);
  }

  .ai-hero-carousel-nav-prev-{{ ai_gen_id }} {
    left: {{ block.settings.arrow_offset }}px;
  }

  .ai-hero-carousel-nav-next-{{ ai_gen_id }} {
    right: {{ block.settings.arrow_offset }}px;
  }

  .ai-hero-carousel-nav-{{ ai_gen_id }} svg {
    width: 24px;
    height: 24px;
    stroke: {{ block.settings.arrow_color }};
  }

  .ai-hero-carousel-dots-{{ ai_gen_id }} {
    position: absolute;
    bottom: {{ block.settings.dots_offset }}px;
    left: 50%;
    transform: translateX(-50%);
    display: flex;
    gap: 12px;
    z-index: 4;
  }

  .ai-hero-carousel-dot-{{ ai_gen_id }} {
    width: {{ block.settings.dot_size }}px;
    height: {{ block.settings.dot_size }}px;
    border-radius: 50%;
    background-color: {{ block.settings.dot_color }};
    border: none;
    cursor: pointer;
    transition: all 0.3s ease;
    opacity: {{ block.settings.dot_opacity | divided_by: 100.0 }};
  }

  .ai-hero-carousel-dot-{{ ai_gen_id }}.active {
    opacity: 1;
    transform: scale(1.2);
    background-color: {{ block.settings.dot_active_color }};
  }

  @media screen and (max-width: 749px) {
    .ai-hero-carousel-slides-{{ ai_gen_id }} {
      height: {{ block.settings.slide_height_mobile }}px;
    }

    .ai-hero-carousel-heading-{{ ai_gen_id }} {
      font-size: {{ block.settings.heading_size | times: 0.6 }}px;
      margin-bottom: 12px;
    }

    .ai-hero-carousel-subheading-{{ ai_gen_id }} {
      font-size: {{ block.settings.subheading_size | times: 0.7 }}px;
      margin-bottom: 16px;
    }

    .ai-hero-carousel-button-{{ ai_gen_id }} {
      font-size: {{ block.settings.button_font_size | times: 0.9 }}px;
      padding: {{ block.settings.button_padding_vertical | times: 0.8 }}px {{ block.settings.button_padding_horizontal | times: 0.8 }}px;
    }

    .ai-hero-carousel-nav-{{ ai_gen_id }} {
      width: {{ block.settings.arrow_size | times: 0.8 }}px;
      height: {{ block.settings.arrow_size | times: 0.8 }}px;
    }

    .ai-hero-carousel-nav-prev-{{ ai_gen_id }} {
      left: {{ block.settings.arrow_offset | times: 0.5 }}px;
    }

    .ai-hero-carousel-nav-next-{{ ai_gen_id }} {
      right: {{ block.settings.arrow_offset | times: 0.5 }}px;
    }

    .ai-hero-carousel-dots-{{ ai_gen_id }} {
      bottom: {{ block.settings.dots_offset | times: 0.7 }}px;
    }
  }
{% endstyle %}

<hero-carousel-{{ ai_gen_id }}
  class="ai-hero-carousel-{{ ai_gen_id }}"
  data-autoplay="{{ block.settings.autoplay }}"
  data-autoplay-speed="{{ block.settings.autoplay_speed }}"
  {{ block.shopify_attributes }}
>
  <div class="ai-hero-carousel-slides-{{ ai_gen_id }}">
    {% for i in (1..4) %}
      {% liquid
        assign slide_enabled_key = 'slide_' | append: i | append: '_enabled'
        assign slide_image_key = 'slide_' | append: i | append: '_image'
        assign slide_heading_key = 'slide_' | append: i | append: '_heading'
        assign slide_subheading_key = 'slide_' | append: i | append: '_subheading'
        assign slide_button_text_key = 'slide_' | append: i | append: '_button_text'
        assign slide_button_link_key = 'slide_' | append: i | append: '_button_link'

        assign slide_enabled = block.settings[slide_enabled_key]
        assign slide_image = block.settings[slide_image_key]
        assign slide_heading = block.settings[slide_heading_key]
        assign slide_subheading = block.settings[slide_subheading_key]
        assign slide_button_text = block.settings[slide_button_text_key]
        assign slide_button_link = block.settings[slide_button_link_key]
      %}

      {% if slide_enabled %}
        <div
          class="ai-hero-carousel-slide-{{ ai_gen_id }} {% if forloop.first %}active{% endif %}"
          data-slide="{{ i }}"
        >
          {% if slide_image %}
            <img
              src="{{ slide_image | image_url: width: 2000 }}"
              srcset="{{ slide_image | image_url: width: 750 }} 750w,
                      {{ slide_image | image_url: width: 1500 }} 1500w,
                      {{ slide_image | image_url: width: 2000 }} 2000w"
              sizes="100vw"
              alt="{{ slide_image.alt | escape }}"
              loading="{% if forloop.first %}eager{% else %}lazy{% endif %}"
              width="{{ slide_image.width }}"
              height="{{ slide_image.height }}"
              class="ai-hero-carousel-slide-image-{{ ai_gen_id }}"
            >
          {% else %}
            <div class="ai-hero-carousel-slide-placeholder-{{ ai_gen_id }}">
              {{ 'hero-apparel-1' | placeholder_svg_tag }}
              <div class="ai-hero-carousel-empty-state-{{ ai_gen_id }}">
                Add an image for slide {{ i }}
              </div>
            </div>
          {% endif %}

          <div class="ai-hero-carousel-overlay-{{ ai_gen_id }}"></div>

          <div class="ai-hero-carousel-content-{{ ai_gen_id }}">
            {% if slide_heading != blank %}
              <h2 class="ai-hero-carousel-heading-{{ ai_gen_id }}">{{ slide_heading }}</h2>
            {% endif %}

            {% if slide_subheading != blank %}
              <p class="ai-hero-carousel-subheading-{{ ai_gen_id }}">{{ slide_subheading }}</p>
            {% endif %}

            {% if slide_button_text != blank %}
              <a
                href="{{ slide_button_link | default: '#' }}"
                class="ai-hero-carousel-button-{{ ai_gen_id }}"
              >
                {{ slide_button_text }}
              </a>
            {% endif %}
          </div>
        </div>
      {% endif %}
    {% endfor %}
  </div>

  {% if block.settings.show_arrows %}
    <button
      class="ai-hero-carousel-nav-{{ ai_gen_id }} ai-hero-carousel-nav-prev-{{ ai_gen_id }}"
      aria-label="Previous slide"
    >
      <svg
        xmlns="http://www.w3.org/2000/svg"
        fill="none"
        viewBox="0 0 24 24"
        stroke-width="2"
        stroke="currentColor"
      >
        <path stroke-linecap="round" stroke-linejoin="round" d="M15 19l-7-7 7-7" />
      </svg>
    </button>

    <button
      class="ai-hero-carousel-nav-{{ ai_gen_id }} ai-hero-carousel-nav-next-{{ ai_gen_id }}"
      aria-label="Next slide"
    >
      <svg
        xmlns="http://www.w3.org/2000/svg"
        fill="none"
        viewBox="0 0 24 24"
        stroke-width="2"
        stroke="currentColor"
      >
        <path stroke-linecap="round" stroke-linejoin="round" d="M9 5l7 7-7 7" />
      </svg>
    </button>
  {% endif %}

  {% if block.settings.show_dots %}
    <div class="ai-hero-carousel-dots-{{ ai_gen_id }}">
      {% for i in (1..4) %}
        {% liquid
          assign slide_enabled_key = 'slide_' | append: i | append: '_enabled'
          assign slide_enabled = block.settings[slide_enabled_key]
        %}
        {% if slide_enabled %}
          <button
            class="ai-hero-carousel-dot-{{ ai_gen_id }} {% if forloop.first %}active{% endif %}"
            data-slide="{{ i }}"
            aria-label="Go to slide {{ i }}"
          ></button>
        {% endif %}
      {% endfor %}
    </div>
  {% endif %}
</hero-carousel-{{ ai_gen_id }}>

<script>
  (function() {
    class HeroCarousel{{ ai_gen_id }} extends HTMLElement {
      constructor() {
        super();
        this.currentSlide = 1;
        this.autoplayInterval = null;
      }

      connectedCallback() {
        this.slides = this.querySelectorAll('.ai-hero-carousel-slide-{{ ai_gen_id }}');
        this.dots = this.querySelectorAll('.ai-hero-carousel-dot-{{ ai_gen_id }}');
        this.prevButton = this.querySelector('.ai-hero-carousel-nav-prev-{{ ai_gen_id }}');
        this.nextButton = this.querySelector('.ai-hero-carousel-nav-next-{{ ai_gen_id }}');
        this.autoplay = this.dataset.autoplay === 'true';
        this.autoplaySpeed = parseInt(this.dataset.autoplaySpeed) || 5000;

        this.enabledSlides = Array.from(this.slides).filter(slide => {
          return slide.dataset.slide;
        });

        this.totalSlides = this.enabledSlides.length;

        if (this.totalSlides > 0) {
          this.setupEventListeners();
          if (this.autoplay) {
            this.startAutoplay();
          }
        }
      }

      setupEventListeners() {
        if (this.prevButton) {
          this.prevButton.addEventListener('click', () => this.previousSlide());
        }

        if (this.nextButton) {
          this.nextButton.addEventListener('click', () => this.nextSlide());
        }

        this.dots.forEach((dot, index) => {
          dot.addEventListener('click', () => {
            this.goToSlide(index + 1);
          });
        });

        this.addEventListener('mouseenter', () => this.stopAutoplay());
        this.addEventListener('mouseleave', () => {
          if (this.autoplay) {
            this.startAutoplay();
          }
        });
      }

      goToSlide(slideNumber) {
        this.slides.forEach(slide => slide.classList.remove('active'));
        this.dots.forEach(dot => dot.classList.remove('active'));

        const targetSlide = this.enabledSlides[slideNumber - 1];
        if (targetSlide) {
          targetSlide.classList.add('active');
        }

        const targetDot = this.dots[slideNumber - 1];
        if (targetDot) {
          targetDot.classList.add('active');
        }

        this.currentSlide = slideNumber;
      }

      nextSlide() {
        let nextSlide = this.currentSlide + 1;
        if (nextSlide > this.totalSlides) {
          nextSlide = 1;
        }
        this.goToSlide(nextSlide);
      }

      previousSlide() {
        let prevSlide = this.currentSlide - 1;
        if (prevSlide < 1) {
          prevSlide = this.totalSlides;
        }
        this.goToSlide(prevSlide);
      }

      startAutoplay() {
        this.stopAutoplay();
        this.autoplayInterval = setInterval(() => {
          this.nextSlide();
        }, this.autoplaySpeed);
      }

      stopAutoplay() {
        if (this.autoplayInterval) {
          clearInterval(this.autoplayInterval);
          this.autoplayInterval = null;
        }
      }

      disconnectedCallback() {
        this.stopAutoplay();
      }
    }

    customElements.define('hero-carousel-{{ ai_gen_id }}', HeroCarousel{{ ai_gen_id }});
  })();
</script>

